  m_Name: "9\u91D1\u8272\u7684\u9C7C\u94A9"
  m_EditorClassIdentifier: 
  historicalPicture: {fileID: 21300000, guid: 53b8df360d014ae4d97e52d770437578, type: 3}
  historicalFactHead: "\u91D1\u8272\u7684\u9C7C\u94A9"
  historicalFactContent: "\u300A\u91D1\u8272\u7684\u9C7C\u94A9\u300B\u8BB2\u8FF0\u4E86\u7EA2\u519B\u957F\u5F81\u9014\u4E2D\uFF0C\u4E00\u4F4D\u708A\u4E8B\u73ED\u957F\u63A5\u53D7\u515A\u7EC4\u7EC7\u4EA4\u7ED9\u7684\u4EFB\u52A1\uFF0C\u7167\u987E\u4E09\u4E2A\u751F\u75C5\u7684\u5C0F\u6218\u58EB\u8FC7\u8349\u5730\u800C\u4E0D\u60DC\u727A\u7272\u81EA\u5DF1\u7684\u6545\u4E8B\uFF0C\u8868\u73B0\u4E86\u7EA2\u519B\u6218\u58EB\u5FE0\u4E8E\u9769\u547D\u3001\u820D\u5DF1\u4E3A\u4EBA\u7684\u5D07\u9AD8\u54C1\u8D28\u3002\u6587\u7AE0\u662F\u6309\u7167\u4E8B\u60C5\u53D1\u5C55\u7684\u987A\u5E8F\u6765\u5199\u7684\uFF0C\u5148\u4EA4\u4EE3\u4E86\u6545\u4E8B\u53D1\u751F\u7684\u80CC\u666F\u548C\u6545\u4E8B\u7684\u4E3B\u8981\u4EBA\u7269\uFF0C\u63A5\u7740\u5199\u4ED6\u4EEC\u4E00\u884C\u4EBA\u53D7\u5230\u9965\u997F\u7684\u5A01\u80C1\uFF0C\u8001\u73ED\u957F\u975E\u5E38\u7740\u6025\uFF0C\u540E\u6765\uFF0C\u8001\u73ED\u957F\u5F00\u59CB\u7ED9\u75C5\u53F7\u4EEC\u9493\u9C7C\uFF0C\u589E\u52A0\u8425\u517B\uFF0C\u800C\u81EA\u5DF1\u5374\u7EE7\u7EED\u5FCD\u53D7\u7740\u9965\u997F\u7684\u6298\u78E8\u751A\u81F3\u4EE5\u5403\u5269\u4E0B\u7684\u8349\u6839\u548C\u9C7C\u9AA8\u5934\u5145\u9965\uFF0C\u6700\u540E\u8001\u73ED\u957F\u5012\u4E0B\u4E86\u3002\n\n<b>\u4EBA\u7269\u539F\u578B</b>\n1935\u5E7411\u6708\uFF0C\u7EA2\u4E8C\u65B9\u9762\u519B\u89E3\u653E\u4E86\u9ED4\u897F\u53BF\u57CE\uFF0C\u738B\u7389\u6E05\u53C2\u52A0\u7EA2\u519B\u3002\n\u8FC7\u8349\u5730\u65F6\uFF0C\u738B\u7389\u6E05\u60F3\u5230\u4E00\u4E2A\u529E\u6CD5\uFF0C\u90E8\u961F\u53D1\u7ED9\u6218\u58EB\u4EEC\u6574\u7406\u5185\u52A1\u7F1D\u8865\u8863\u7269\u7684\u7F1D\u8863\u9488\uFF0C\u70E7\u5F2F\u4E86\u505A\u6210\u9C7C\u94A9\uFF0C\u6CA1\u591A\u4E45\u5C31\u9493\u5230\u4E24\u6761\u534A\u65A4\u91CD\u7684\u9C7C\u3002\u518D\u627E\u6765\u67AF\u679D\uFF0C\u5347\u8D77\u706B\uFF0C\u7528\u8138\u76C6\u716E\u8D77\u9C7C\u6C64\u6765\u3002\u9C7C\u9999\u628A\u540C\u5FD7\u4EEC\u5438\u5F15\u8FC7\u6765\uFF0C\u6BCF\u4EBA\u90FD\u5206\u5230\u4E86\u4E00\u7897\u9C7C\u6C64\u3002\u540E\u6765\uFF0C\u6218\u53CB\u4EEC\u4E5F\u5B66\u7740\u738B\u7389\u6E05\u7684\u529E\u6CD5\u9493\u5230\u4E86\u9C7C\u3002\n\u89E3\u653E\u540E\uFF0C\u8001\u7EA2\u519B\u738B\u7389\u6E05\u628A\u73CD\u85CF\u591A\u5E74\uFF0C\u5DF2\u7ECF\u957F\u6EE1\u4E86\u7EA2\u9508\u7684\u9C7C\u94A9\uFF0C\u9001\u5230\u4E86\u519B\u535A\uFF0C\u8FD9\u5C31\u662F\u4ECA\u5929\u6211\u4EEC\u5728\u519B\u535A\u770B\u5230\u7684\u90A3\u679A\u91D1\u8272\u7684\u9C7C\u94A9\uFF0C\u4E5F\u5C31\u662F\u8BFE\u6587\u300A\u91D1\u8272\u7684\u9C7C\u94A9\u300B\u7684\u539F\u578B\u3002"
