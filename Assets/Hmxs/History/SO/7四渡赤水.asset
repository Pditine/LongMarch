  m_Name: "7\u56DB\u6E21\u8D64\u6C34"
  m_EditorClassIdentifier: 
  historicalPicture: {fileID: 21300000, guid: 09fb3b51bac1b31449cbf3c7f190719d, type: 3}
  historicalFactHead: "\u56DB\u6E21\u8D64\u6C34"
  historicalFactContent: "\u65F6\u95F4\uFF1A1935\u5E74\n\u5730\u70B9\uFF1A\u4F4D\u4E8E\u4ECA\u5929\u7684\u8D35\u5DDE\u7701\u8D64\u6C34\u6CB3\u6D41\u57DF\u3002\n\n<b>\u80CC\u666F</b>\n\u8FD9\u6B21\u884C\u52A8\u53D1\u751F\u5728\u4E2D\u56FD\u5171\u4EA7\u515A\u9886\u5BFC\u4E0B\u7684\u7EA2\u519B\u5728\u957F\u5F81\u9014\u4E2D\uFF0C\u4E3A\u4E86\u8EB2\u907F\u56FD\u6C11\u515A\u519B\u961F\u7684\u56F4\u8FFD\u5835\u622A\u3002\n\n<b>\u6218\u7565\u76EE\u7684</b>\n\u56DB\u6E21\u8D64\u6C34\u7684\u4E3B\u8981\u76EE\u7684\u662F\u4E3A\u4E86\u8FF7\u60D1\u8FFD\u51FB\u7684\u56FD\u6C11\u515A\u519B\u961F\uFF0C\u6446\u8131\u654C\u519B\u7684\u56F4\u8FFD\u5835\u622A\uFF0C\u4E3A\u7EA2\u519B\u7EE7\u7EED\u957F\u5F81\u521B\u9020\u6761\u4EF6\u3002\u7EA2\u519B\u5728\u8D64\u6C34\u6CB3\u533A\u57DF\u6267\u884C\u4E86\u56DB\u6B21\u6E21\u6CB3\u884C\u52A8\uFF0C\u6BCF\u6B21\u6E21\u6CB3\u90FD\u91C7\u53D6\u4E0D\u540C\u7684\u6218\u672F\u548C\u8DEF\u7EBF\uFF0C\u6210\u529F\u8FF7\u60D1\u4E86\u654C\u519B\u3002\u7EA2\u519B\u5229\u7528\u590D\u6742\u7684\u5730\u5F62\u548C\u5DE7\u5999\u7684\u6218\u672F\uFF0C\u6709\u6548\u5730\u89C4\u907F\u4E86\u654C\u519B\u7684\u4E3B\u529B\uFF0C\u5E76\u4E14\u5728\u591A\u6B21\u6218\u6597\u4E2D\u53D6\u5F97\u4E86\u80DC\u5229\u3002\n\n<b>\u6218\u7565\u610F\u4E49</b>\n\u56DB\u6E21\u8D64\u6C34\u88AB\u8BA4\u4E3A\u662F\u957F\u5F81\u671F\u95F4\u7EA2\u519B\u6307\u6325\u827A\u672F\u7684\u6770\u51FA\u5C55\u793A\uFF0C\u663E\u793A\u4E86\u6BDB\u6CFD\u4E1C\u7B49\u9886\u5BFC\u4EBA\u7684\u519B\u4E8B\u624D\u80FD\u3002\u8FD9\u6B21\u884C\u52A8\u662F\u957F\u5F81\u8FC7\u7A0B\u4E2D\u7684\u4E00\u4E2A\u91CD\u8981\u8F6C\u6298\u70B9\uFF0C\u4E3A\u7EA2\u519B\u6446\u8131\u56F0\u5883\u3001\u7EE7\u7EED\u957F\u5F81\u5317\u4E0A\u6253\u4E0B\u4E86\u57FA\u7840\u3002\n\n<b>\u5F71\u54CD</b>\n\u56DB\u6E21\u8D64\u6C34\u7684\u6210\u529F\u6781\u5927\u5730\u63D0\u5347\u4E86\u7EA2\u519B\u58EB\u5175\u7684\u58EB\u6C14\uFF0C\u589E\u5F3A\u4E86\u5BF9\u672A\u6765\u80DC\u5229\u7684\u4FE1\u5FC3\u3002\u56DB\u6E21\u8D64\u6C34\u6210\u4E3A\u4E2D\u56FD\u9769\u547D\u53F2\u4E0A\u7684\u4E00\u4E2A\u91CD\u8981\u4E8B\u4EF6\uFF0C\u8C61\u5F81\u7740\u7EA2\u519B\u5728\u9006\u5883\u4E2D\u7684\u575A\u97E7\u548C\u667A\u6167\u3002"
