  m_Name: "6\u9075\u4E49\u4F1A\u8BAE"
  m_EditorClassIdentifier: 
  historicalPicture: {fileID: 21300000, guid: a5eb5babbce8d304494781457bb43068, type: 3}
  historicalFactHead: "\u9075\u4E49\u4F1A\u8BAE"
  historicalFactContent: "\u65F6\u95F4\uFF1A1935\u5E741\u6708\n\u5730\u70B9\uFF1A\u4F4D\u4E8E\u8D35\u5DDE\u7701\u9075\u4E49\u5E02\u7684\u4E00\u6240\u5973\u5B50\u5B66\u6821\u3002\n\n<b>\u80CC\u666F</b>\n\u957F\u5F81\u9014\u4E2D\uFF1A\u6B64\u65F6\u7684\u7EA2\u519B\u6B63\u5904\u4E8E\u957F\u5F81\u9014\u4E2D\uFF0C\u9762\u4E34\u56FD\u6C11\u515A\u519B\u961F\u7684\u4E25\u5BC6\u56F4\u8FFD\u5835\u622A\u3002\n\u6218\u7565\u56F0\u5883\uFF1A\u7531\u4E8E\u4E4B\u524D\u201C\u5DE6\u201D\u503E\u7684\u519B\u4E8B\u6307\u6325\u9519\u8BEF\uFF0C\u7EA2\u519B\u906D\u53D7\u4E86\u91CD\u5927\u635F\u5931\uFF0C\u8FEB\u5207\u9700\u8981\u8C03\u6574\u6307\u6325\u7B56\u7565\u548C\u6218\u672F\u3002\n\n<b>\u4F1A\u8BAE\u76EE\u7684</b>\n\u6218\u7565\u8C03\u6574\uFF1A\u5206\u6790\u548C\u4FEE\u6B63\u4E4B\u524D\u7684\u6218\u7565\u51B3\u7B56\uFF0C\u91CD\u65B0\u89C4\u5212\u957F\u5F81\u7684\u8DEF\u7EBF\u548C\u6218\u672F\u3002\n\u9886\u5BFC\u5C42\u53D8\u52A8\uFF1A\u89E3\u51B3\u7EA2\u519B\u5185\u90E8\u7684\u9886\u5BFC\u95EE\u9898\uFF0C\u7279\u522B\u662F\u519B\u4E8B\u6307\u6325\u4E0A\u7684\u95EE\u9898\u3002\n\n<b>\u4E3B\u8981\u8BA8\u8BBA\u5185\u5BB9</b>\n\u53CD\u601D\u4E4B\u524D\u7B56\u7565\uFF1A\u5BF9\u201C\u5DE6\u201D\u503E\u519B\u4E8B\u8DEF\u7EBF\u7684\u9519\u8BEF\u8FDB\u884C\u6279\u8BC4\u548C\u53CD\u601D\u3002\n\u8C03\u6574\u6307\u6325\u4F53\u7CFB\uFF1A\u8C03\u6574\u7EA2\u519B\u7684\u6307\u6325\u4F53\u7CFB\uFF0C\u6BDB\u6CFD\u4E1C\u548C\u5468\u6069\u6765\u7B49\u4EBA\u7684\u519B\u4E8B\u5730\u4F4D\u5F97\u5230\u52A0\u5F3A\u3002\n\u5236\u5B9A\u65B0\u6218\u7565\uFF1A\u51B3\u5B9A\u91C7\u53D6\u66F4\u7075\u6D3B\u3001\u66F4\u5177\u6709\u6218\u7565\u6027\u7684\u884C\u52A8\u8BA1\u5212\u3002\n\n<b>\u7ED3\u679C\u4E0E\u5F71\u54CD</b>\n\u6BDB\u6CFD\u4E1C\u7684\u5D1B\u8D77\uFF1A\u6BDB\u6CFD\u4E1C\u6210\u4E3A\u5B9E\u9645\u7684\u519B\u4E8B\u6307\u6325\u9886\u5BFC\uFF0C\u8FD9\u4E00\u51B3\u7B56\u5BF9\u7EA2\u519B\u540E\u7EED\u7684\u884C\u52A8\u548C\u6574\u4E2A\u4E2D\u56FD\u9769\u547D\u90FD\u4EA7\u751F\u4E86\u6DF1\u8FDC\u7684\u5F71\u54CD\u3002\n\u6218\u7565\u8F6C\u53D8\uFF1A\u4F1A\u8BAE\u4E4B\u540E\uFF0C\u7EA2\u519B\u7684\u884C\u52A8\u66F4\u52A0\u7075\u6D3B\uFF0C\u6210\u529F\u907F\u514D\u4E0E\u56FD\u6C11\u515A\u519B\u961F\u7684\u6B63\u9762\u51B2\u7A81\uFF0C\u4E3A\u957F\u5F81\u7684\u7EE7\u7EED\u63D0\u4F9B\u4E86\u6761\u4EF6\u3002\n\u515A\u7684\u53D1\u5C55\uFF1A\u4F1A\u8BAE\u6807\u5FD7\u7740\u4E2D\u56FD\u5171\u4EA7\u515A\u5728\u6781\u7AEF\u56F0\u5883\u4E2D\u7684\u81EA\u6211\u6551\u8D4E\u548C\u6210\u719F\uFF0C\u4E3A\u540E\u7EED\u7684\u9769\u547D\u6D3B\u52A8\u5960\u5B9A\u4E86\u575A\u5B9E\u57FA\u7840\u3002\n\u9075\u4E49\u4F1A\u8BAE\u662F\u4E2D\u56FD\u5171\u4EA7\u515A\u5386\u53F2\u4E0A\u7684\u8F6C\u6298\u70B9\u4E4B\u4E00\uFF0C\u5B83\u4E0D\u4EC5\u633D\u6551\u4E86\u7EA2\u519B\u4E8E\u5371\u96BE\u4E4B\u4E2D\uFF0C\u4E5F\u4E3A\u4E2D\u56FD\u5171\u4EA7\u515A\u7684\u672A\u6765\u53D1\u5C55\u65B9\u5411\u505A\u51FA\u4E86\u5173\u952E\u6027\u7684\u8C03\u6574\u3002"
